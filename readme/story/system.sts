* hyperlink = function (@урл, @текст) {
  return "[*@текст;](*@урл;)";
}

* localPhoto = function (@урл, @текст, @imgRoot) {
  if (!@imgRoot) {
    @imgRoot = "https:\//raw.githubusercontent.com/freewebtime/storytailor-vscode-extension/master/client/lib/images/";
  }
  return "![*@текст;](*@imgRoot;*@урл;)";
}